<Window x:Class="Sharpness.Disassembler"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sharpness"
        mc:Ignorable="d"
        Title="Disassembler" Height="344.525" Width="615.073">
    <Grid Background="#FF363636">
        <ListView x:Name="memoryList" HorizontalAlignment="Left" Height="267" Margin="10,28,0,0" VerticalAlignment="Top" Width="231">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Address            " DisplayMemberBinding="{Binding Address}" />
                    <GridViewColumn Header="Data               " DisplayMemberBinding="{Binding Data}"/>
                </GridView>
            </ListView.View>
        </ListView>
        <Label Content="Memory" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top" Foreground="#FFCECECE"/>
        <Label Content="PC: " HorizontalAlignment="Left" Margin="269,28,0,0" VerticalAlignment="Top" Foreground="#FFECECEC"/>
        <TextBox x:Name="PCBox" HorizontalAlignment="Left" Height="22" Margin="297,30,0,0" TextWrapping="Wrap" Text="FF" VerticalAlignment="Top" Width="47"/>

    </Grid>
</Window>
