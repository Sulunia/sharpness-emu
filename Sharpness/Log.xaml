<Window x:Class="Sharpness.Log"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sharpness"
        mc:Ignorable="d"
        Title="Log" Height="300" Width="300" ResizeMode="NoResize" ShowInTaskbar="False" Loaded="Window_Loaded">
    <Grid Background="#FF232323">
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Log text" VerticalAlignment="Top" Height="250" Width="273" Foreground="#FF92CAEC" FontFamily="Courier New" FontSize="12"/>

    </Grid>
</Window>
